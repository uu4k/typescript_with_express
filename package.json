    "babel-jest": "^26.0.1",
    "jest": "^26.0.1",
    "nodemon": "^2.0.4",
    "npm-run-all": "^4.1.5",
    "sequelize-cli": "^5.5.1",
    "ts-node": "^8.10.1",
    "ts-node-dev": "^1.0.0-pre.44",
    "tsconfig-paths": "^3.9.0",
    "tslint": "^6.1.2",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^3.9.3"
  },
  "dependencies": {
    "dotenv": "^8.2.0",
    "ejs": "^3.1.3",
    "express": "^4.17.1",
    "express-ejs-layouts": "^2.5.0",
    "mysql2": "^2.1.0",
    "sequelize": "^5.21.10"
